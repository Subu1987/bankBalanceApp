<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <SelectDialog
        title="Select Ledger"
        items="{yearData>/}"
        search="_handleValueFiscalYearSearch"
        confirm="_handleValueDialogFiscalYearClose"
        cancel="_handleValueDialogFiscalYearClose"
        contentWidth="auto"
        class="sapUiPopupWithPadding">
